QLabel{
    color: white;

}

QPushButton {
    background-color: #049ff1;
    color: white;
    border-width: 2px;
    border-color: white;
    border-style: solid;
    border-radius: 5;
    padding: 3px;
    min-width: 9ex;
    min-height: 2.5ex;
}

QPushButton:hover {
    padding-right: 5px;
    padding-bottom: 5px;
    background-color: #72CF07;
}

/* Increase the padding, so the text is shifted when the button is
   pressed. */
QPushButton:pressed {
    padding-left: 5px;
    padding-top: 5px;
    background-color: #58b2dc;
}


